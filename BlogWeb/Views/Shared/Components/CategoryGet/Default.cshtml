@model IEnumerable<BlogWeb.DL.Models.Category>

<div class="single-sidebar-widget tag_cloud_widget">
    <h4 class="single-sidebar-widget__title">Kategoriler</h4>
    <ul class="list">
        @foreach (var item in Model)
        {
            <li>
                <a href="~/Home/HomeCategory/@item.CategoryId">@item.CategoryName</a>
            </li>
        }
      
    </ul>
</div>
