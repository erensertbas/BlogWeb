    <nav class="sidebar vertical-scroll  ps-container ps-theme-default ps-active-y">
        <div class="logo d-flex justify-content-between">
            <a href="~/Yonetici"><img src="~/images/logo1.png" alt="" style="max-height:100px;"></a>
            <div class="sidebar_close_icon d-lg-none">
                <i class="ti-close"></i>
            </div>
        </div>
        <ul id="sidebar_menu">
            <li class="mm-active" ><a href="~/Anasayfa">Anasayfa</a></li>


              <li class="mm-active" ><a href="~/Yonetici">Panel</a></li>

            <li class="mm-active">
                <a class="has-arrow" aria-expanded="false">
                    <span>Makaleler</span>
                </a>
                <ul>
                    <li><a href="~/Makaleler">Tüm Makaleler</a></li>
                    <li><a href="~/MakaleOnayListesi">Onay Bekleyenler</a></li>
                    <li><a href="~/Makalelerim">Makalelerim</a></li>
